{
  "name": "@folio/service-interaction",
  "version": "1.1.0",
  "description": "FOLIO app for service-interaction",
  "main": "src/index.js",
  "repository": "folio-org/ui-service-interaction",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "lint": "eslint .",
    "test": "yarn run test:jest",
    "test:jest": "jest --ci --coverage",
    "build-mod-descriptor": "stripes mod descriptor --full --strict | jq '.[]' > module-descriptor.json ",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-service-interaction ./translations/ui-service-interaction/compiled"
  },
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-service-interaction.meta.title",
    "route": "/service-interaction",
    "home": "/service-interaction",
    "hasSettings": true,
    "queryResource": "query",
    "okapiInterfaces": {
      "servint": "2.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.service-interaction.enabled",
        "displayName": "UI: ui-service-interaction module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.service-interaction.enabled",
        "displayName": "Settings (service-interaction): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ],
        "visible": true
      }
    ]
  },
  "dependencies": {
    "@k-int/stripes-kint-components": "^4.0.0",
    "dom-helpers": "^3.4.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.0",
    "react-final-form": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.18.6",
    "@babel/plugin-proposal-throw-expressions": "^7.18.6",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@folio/eslint-config-stripes": "^6.1.0",
    "@folio/stripes": "^7.3.0 || ^8.0.0",
    "@folio/stripes-cli": "^2.6.0",
    "@folio/stripes-erm-testing": "^1.0.0",
    "@folio/stripes-testing": "^4.2.0",
    "@formatjs/cli": "^4.2.31",
    "@testing-library/dom": "^8.16.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.3.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "core-js": "^3.6.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-testing-library": "^5.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^28.1.2",
    "jest-junit": "^13.2.0",
    "jest-webpack": "^0.5.1",
    "moment": "^2.22.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.1",
    "react-query": "^3.6.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^7.5.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.3.0 || ^8.0.0",
    "moment": "^2.22.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.1",
    "react-router-dom": "^5.2.0"
  }
}
