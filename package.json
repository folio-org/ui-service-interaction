{
  "name": "@folio/service-interaction",
  "version": "1.0.0",
  "description": "FOLIO app for service-interaction",
  "main": "src/index.js",
  "repository": "folio-org/ui-service-interaction",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output ./output",
    "lint": "eslint .",
    "test": "yarn run test:jest",
    "test:jest": "jest --ci --coverage --passWithNoTests",
    "formatjs-compile": "formatjs compile-folder --ast --format simple ./translations/ui-service-interaction ./translations/ui-service-interaction/compiled"
  },
  "stripes": {
    "actsAs": [
      "settings"
    ],
    "displayName": "ui-service-interaction.meta.title",
    "route": "/service-interaction",
    "home": "/service-interaction",
    "hasSettings": true,
    "queryResource": "query",
    "okapiInterfaces": {
      "remote-sync": "1.0"
    },
    "permissionSets": [
      {
        "permissionName": "module.service-interaction.enabled",
        "displayName": "UI: ui-service-interaction module is enabled"
      },
      {
        "permissionName": "settings.service-interaction.enabled",
        "displayName": "Settings (service-interaction): display list of settings pages",
        "subPermissions": [
          "settings.enabled"
        ]
      }
    ]
  },
  "stripesDeps": [
    "@k-int/stripes-kint-components"
  ],
  "dependencies": {
    "prop-types": "^15.6.0",
    "@k-int/stripes-kint-components": "^2.4.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@bigtest/interactor": "^0.9.3",
    "@bigtest/mocha": "^0.5.2",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "^6.1.0",
    "@folio/stripes": "^7.1.0",
    "@folio/stripes-erm-components": "^6.1.0",
    "@folio/stripes-cli": "^2.5.0",
    "@folio/stripes-testing": "^4.2.0",
    "@formatjs/cli": "^4.2.31",
    "@testing-library/dom": "^7.26.6",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "@babel/eslint-parser": "^7.15.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "core-js": "^3.6.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.2.4",
    "eslint-plugin-testing-library": "^3.10.0",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "inflected": "^2.0.4",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "jest-junit": "^12.0.0",
    "jest-webpack": "^0.5.1",
    "miragejs": "^0.1.40",
    "mocha": "^6.2.2",
    "moment": "^2.22.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.1",
    "react-query": "^3.6.0",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^7.5.0"
  },
  "peerDependencies": {
    "@folio/stripes": "^7.1.0",
    "moment": "^2.22.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.1",
    "react-router-dom": "^5.2.0"
  }
}
